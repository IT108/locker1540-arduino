int Num;
int Num1;
int w[8];
int w1[8];

void setup() {
  Serial.begin(9600);
  Serial3.begin(115200);

}
void writeNew(int q[]){
  for (int i = 0; i < 8; i++) {
    Serial.write(q[i]);
  }
}
void loop() {
  if (Serial3.available()) {
    int a = Serial3.read();
    if (Num1 >= 1) {
      w1[Num1 - 1] = a;
      Num1++;
    }
    if (a == 126) {
      Num1 = 1;
    }
    if (Num1 == 9) {
      writeNew(w1);
    }
  } else {
    Num1 = 0;
  }
}
